{
    "version": 2,
    "functions": {
        "api/chatbot.ts": {
            "runtime": "nodejs18.x",
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/api/chatbot",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://gs926.com.ar",
                "Access-Control-Allow-Methods": "POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type"
            },
            "dest": "/api/chatbot.ts"
        }
    ]
}